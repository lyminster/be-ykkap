@model IEnumerable<MainProject.Models.UserViewModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLTE/_Layout.cshtml";
}


<div>

    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-12">
                    @*<h1 class="m-0 text-dark">Welcome to YKKAP Application</h1>*@
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <section class="content">
        <div class="container-fluid">
            <!-- new vendor form -->
            <div class="row center" >
                <div class="col-md-12">
                    <div class="card card-info">
                         
                        <div class="card-body">
                            <h1 class="m-0 text-dark"><img src="~/img/hands.png" />&nbsp;Welcome!</h1>
                            <br/>
                            <h4>Welcome to YKKAP YES Catalog Dashboard</h4>
                        </div>
                    </div>
                </div>
            </div>
            <!-- new vendor form -->

        </div><!-- /.container-fluid -->
    </section>

</div>



@section myScripts{
    <script src="~/plugins/jquery/jquery.min.js"></script>
    <script>
        $(document).ready(function () {

            $("form").submit(function () {

                ShowLoading();

            });

            @if (TempData["notification"] != null)
            {
                @Html.Raw(TempData["notification"])
            }

        });

    </script>

}